<UserControl
    x:Class="FlexTrader.MVVM.Resources.VectorGraphics.TestFigures"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:FlexTrader.MVVM.Resources.VectorGraphics"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="1000"
    d:DesignWidth="1000"
    mc:Ignorable="d">
    <Viewbox Stretch="Uniform">
        <Path
            Width="100"
            Height="100"
            Stroke="White" StrokeThickness="7">
            <Path.Data>
                <GeometryGroup>
                    <GeometryGroup.Children>
                        <LineGeometry StartPoint="20, 40" EndPoint="40, 90" />
                        <LineGeometry StartPoint="80, 10" EndPoint="40, 90" />
                    </GeometryGroup.Children>
                </GeometryGroup>
            </Path.Data>
        </Path>
    </Viewbox>
</UserControl>
